#include<stdio.h>
int main(){
    int n,oct=0;
    printf("Enter number = ");
    scanf("%d",&n);
    int temp=n;
    int place=1;
    while(temp!=0){
        int rem = temp%10;
        oct=oct+rem*place;
        place=place*8;
        temp=temp/10;
    }
    printf("Octal of %d is %d",n,oct);
    return 0;
}
