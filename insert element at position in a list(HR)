#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,temp;
    scanf("%d",&n);
    int list[n];
    for(int i=0;i<n;i++){
        scanf("%d",&list[i]);
    }
    int value,index;
    scanf("%d %d",&value,&index);
    for(int i=index;i<n+1;i++){
       temp=list[i];
        list[i]=value;
        value=temp;
    }
    for(int i=0;i<n+1;i++){
        printf("%d ",list[i]);
    }
    return 0;
}
