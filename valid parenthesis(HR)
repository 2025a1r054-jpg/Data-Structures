#include <stdio.h>
#include <string.h>
#include <stdlib.h>
int main() {
    char str[10001];
    if (scanf("%s", str) != 1) return 0;
    int len = strlen(str);
    int top = -1;
    int flag = 0;
    char *stack = (char *)malloc(len * sizeof(char));
    for (int i = 0; i < len; i++) {
        char ch = str[i];
        if (ch == '(' || ch == '[' || ch == '{') {
            stack[++top] = ch;
        } 
        else if (ch == ')' || ch == ']' || ch == '}') {
            if (top == -1) {
                flag = 1;
                break;
            }
            if ((ch == ')' && stack[top] == '(') ||
                (ch == ']' && stack[top] == '[') ||
                (ch == '}' && stack[top] == '{')) {
                top--;
            } else {
                flag = 1;
                break;
            }
        }
    }
    if (flag == 0 && top == -1) {
        printf("%s is Valid\n", str);
    } else {
        printf("%s is Invalid\n", str);
    }
    free(stack);
    return 0;
}
