#include <stdio.h>
#include <string.h>
int main() {
    char s[100];     
    char result[100];
    int j = 0;       
    scanf("%s", s);
    for (int i = 0; i < strlen(s); i++) {
        if (s[i] == '*') {
            if (j > 0) {
                j--; 
            }
        } else {
            result[j] = s[i];
            j++;
        }
    }
    result[j] = '\0';
    printf("%s\n", result);
    return 0;
}
