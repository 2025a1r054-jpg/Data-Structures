#include <stdio.h>
#include <string.h>

int main() {
    char s[100];
    char result[100]; 
    int j = 0; 
    scanf("%s", s);
    int n = strlen(s);
    for (int i = 0; i < n; i++) {
        if (i == 0 || s[i] != s[i-1]) {
            result[j] = s[i]; 
            j++;             
        }
    }
    result[j] = '\0';
    printf("%s\n", result);
    return 0;
}
