#include<stdio.h>
int main(){
    int n,bin=0;
    printf("Enter the number = ");
    scanf("%d",&n);
    int temp=n;
    int place=1;
    while(temp!=0){
        int rem = temp%2;
        bin = bin+rem*place;
        temp=temp/2;
        place=place*10;
    }
    printf("Binary of %d is %d",n,bin);
    return 0;
}
